 }

}

int main()
{
    int n;
    printf("ENTER CHOICE : ");
    scanf("%d",&n);
    switch(n)
    {
    case 1: letter_A();break;
    case 2: letter_B();break;
    case 3: letter_C();break;
    case 4: letter_D();break;
    case 5: letter_E();break;
    case 6: letter_F();break;
    case 7: letter_H();break;
    case 8: letter_I();break;
    case 10: letter_P();break;
    case 11: letter_S();break;
    case 12: letter_T();break;
    case 13: letter_U();break;
    default : printf("INVALID CHOICE");

    }
    return 0;
}


