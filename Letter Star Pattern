#include <stdio.h>
#include <stdlib.h>

 void letter_A()
{
 int i,j,k;
 for (i=1;i<=7;i++)
 {
     for(j=1;j<=5;j++)
     {
        if( (i==1 && j>1 && j<5) || ((j==1 || j==5) && i>1) || i==4)
        {
            printf("* ");
        }else


           {
               printf("  ");
           }
        }




     printf("\n");
 }
}

void letter_B()
{
 int i,j,k;
 for (i=1;i<=5;i++)
 {
     for(j=1;j<=5;j++)
     {
