#include <stdio.h>
#include <stdlib.h>

int main()
{
   int a[2][2];
   int b[2][2];
   int c[2][2];

   printf("a matrix");
   printf("\n");
   a[0][0]=5;
   a[0][1]=5;
   a[1][0]=5;
   a[1][1]=5;
   printf("%d\t",a[0][0]);
   printf("%d\t",a[0][1]);
   printf("\n");
   printf("%d\t",a[1][0]);
   printf("%d\t",a[1][1]);

   printf("\n");

   printf("b matrix");
   printf("\n");
   b[0][0]=1;
   b[0][1]=1;
   b[1][0]=1;
   b[1][1]=1;
   printf("%d\t",b[0][0]);
   printf("%d\t",b[0][1]);
   printf("\n");
   printf("%d\t",b[1][0]);
   printf("%d\t",b[1][1]);

   printf("\n");

   c[0][0]=a[0][0]+b[0][0];
   c[0][1]=a[0][1]+b[0][1];
   c[1][0]=a[1][0]+b[1][0];
   c[1][1]=a[1][1]+b[1][1];

   printf("sum is ");
   printf("\n");
   printf("%d\t",c[0][0]);
   printf("%d\t",c[0][1]);
   printf("\n");
   printf("%d\t",c[1][0]);
   printf("%d\t",c[1][1]);



   return 0;
}
