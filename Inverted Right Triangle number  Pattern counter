#include <stdio.h>

int main()
{
    int n,m,c;
    printf("Enter the number of rows");
    scanf("%d",&n);
    m=n;
    c=2*n;
   for(int i=1;i<=n;i++)
   {
       for(int j=1;j<i;j++)
       {
           printf(" ");
       }
       for(int k=1;k<=m;k++)
       {
           c--;
           printf("%d",c+1);
       }
       m--;

      printf("\n");
    }
    return 0;
}
