#include <stdio.h>
#include <stdlib.h>

int addition ()
{
    int a,b,c;
    printf("Enter a : ");
    scanf("%d",&a);
    printf("Enter b : ");
    scanf("%d",&b);
    c=a+b;
    printf("Ans : %d",c);
}

int subtraction ()
{
     int a,b,c;
    printf("Enter a : ");
    scanf("%d",&a);
    printf("Enter b : ");
    scanf("%d",&b);
    c=a-b;
    printf("Ans : %d",c);
}



int main()
{
   int a,b,c;
   printf("Enter choice : ");
   scanf("%d",&a);
   switch(a)
   {
       case 1:  addition ();break;
       case 2 :subtraction ();break;
       default: printf("INVALID CHOICE ");
   }
}
