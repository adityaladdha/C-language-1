#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a[3][3],b[3][3],c[3][3],i,j;
int s;
int l,k;
    printf("enter first matrix");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            scanf("%d",&a[i][j]);
        }

    }
    printf("enter second matrix");

    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            scanf("%d",&b[i][j]);
        }

    }
// adition
printf("addition of given matrix is\n");
for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            c[i][j]=a[i][j]+b[i][j];
            printf("%d\t",c[i][j]);
        }
        printf("\n");
    }

s=sizeof(c[0])/sizeof(c[0][0]);
l=sizeof(c)/sizeof(c[0]);
k=s*l;
printf("%d",k);

}




