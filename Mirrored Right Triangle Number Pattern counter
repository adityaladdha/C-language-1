

#include <stdio.h>

int main()
{
    int n,m=1,j,k,c=0;
    printf("Enter the number of rows");
    scanf("%d",&n);
    for(int i=n;i>=1;i--)
    {
        for(j=1;j<=i-1;j++)
        {
          printf(" ");
        }
        for(k=1;k<=m;k++)
        {
            c++;
            printf("%d",c);
        }
        printf("\n");
        m++;
    }
    return 0;
}
